<div>
    {{#with main}}
    <main>
        <section>
            {{> title data="My educationss" big=true}}
            <ul>
                {{#each educations}}
                <li>{{name}}
                    {{#unless isEnd}}
                    (Ще в процесі)
                    {{/unless}}</li>
                {{/each }}
            </ul>
        </section>
        <section>
            {{> title data="My certificatess" }}
            <ul>
                {{#each certificates}}
                <li>
                    {{name}}({{id}})
                </li>
                {{/each }}
            </ul>
        </section>
    </main>
    {{/with}}
</div>