<div>
    <main>
        <section>
            {{#with page_header}}
            <h1 class="text-center">{{title}}</h1><br>
            <p class="text-center">{{description}}</p><br>
            {{/with}} {{! -with_page_header }}

            <div class="card">
                <div class="card-body">


                    {{#with section}}
                    {{#with about_us}}

                    <div class="display-1 text-end">{{title_list.main}}</div><br>
                    <h2 class="text-end">{{title_list.sub}}</h2><br>
                    <button type="button" class="btn btn-outline-info">{{button.text}}</button><br>

                </div>
            </div>
            {{/with}} {{! -with_about_us- }}<br>

            <div class="card">
                <div class="card-body">
                    {{#with advantage}}

                    <div class="display-5 text-muted">{{title}}</div><br>

                    {{#with efficiency}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-muted">{{title}}</div><br>
                            <h6 class="text-muted">{{sub_title}}</h6><br>
                            <p class="text-muted">{{description}}
                            <p><br>
                            <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div><br>
                            <br>
                        </div>
                    </div>
                    {{/with}} {{! -with_efficiency- }}<br>

                    {{#with innovation}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-center text-muted">{{title}}</div><br>
                            <h6 class="text-center text-muted">{{sub_title}}</h6><br>
                            <p class="text-center text-muted">{{description}}
                            <p><br>
                            <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div><br>
                            <br>
                        </div>
                    </div>
                    {{/with}} {{! -with_innovation- }}<br>

                    {{#with collaboration}}
                    <div class="card">
                        <div class="card-body">
                            <div class="display-4 text-end text-muted">{{title}}</div><br>
                            <h6 class="text-end text-muted">{{sub_title}}</h6><br>
                            <p class="text-end text-muted">{{description}}
                            <p><br>
                            <div><button type="button" class="btn btn-outline-warning">{{button}}</button></div><br>
                            <br>
                        </div>
                    </div>
                    {{/with}} {{! -with_collaboration- }}
                </div>
            </div>
            {{/with}} {{! -with_advantage- }}<br><br>


            <div class="card">
                <div class="card-body">
                    {{#with agreement}}
                    <div class="display-5"><b>{{title}}</b></div><br>
                    {{#with info}}
                    <div class="card">
                        <div class="card-body">
                            <h5>{{sub_title}}</h5><br>
                            <div class="card">
                                <div class="card-body">
                                    {{#with detail}}
                                    <p>{{text}}
                                    <p><br>
                                    <div><button type="button" class="btn btn-success">{{button}}</button></div>
                                    <br>
                                    {{/with}} {{! -detail- }}
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/with}} {{! -info- }}
                    {{/with}} {{! -agreement- }}
                </div>
            </div>

            {{/with}} {{! -with_section- }}
            <br>



        </section>
    </main>
</div>