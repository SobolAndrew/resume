<main>
    <div class="row g-5">
        {{#with resumeProject}}

        {{> indexHeader}}

        <div class="col-6">
            <h3 class="p-3">{{title}}</h3>
            {{#each description}}<div class="p-3">
                <p>{{text}}</p>
            </div>
            {{/each}}


            {{#each links}}
            <div class="m-3"><a href="{{href}}" class="btn btn-primary">{{text}}</a></div>
            {{/each}}
        </div>

        {{/with}} {{!-- resumeProject --}}

    </div>

</main>