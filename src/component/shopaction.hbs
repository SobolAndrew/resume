<section>
    {{#with service}}
    <div class="card text-bg-success ">
        <div class="card-body">
            <h2 class="card-title text-center">{{title}}</h2>
            <p class="card-text text-center py-3">{{description}}</p>
            {{#each buttons}}
            <div class="text-center"><a href="{{link}}" class="btn btn-primary  mb-3">{{text}}</a></div>
            {{/each}} {{!-- each_buttons --}}
        </div>
    </div>
    {{/with}} {{!-- with-service --}}
</section>